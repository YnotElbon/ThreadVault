<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.threadvault.maintenance</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-lc</string>
    <string>${HOME}/ThreadVault/system/bin/daily_maintenance.sh</string>
  </array>
  <key>WorkingDirectory</key>
  <string>${HOME}/ThreadVault</string>
  <key>RunAtLoad</key>
  <true/>
  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>2</integer>
    <key>Minute</key>
    <integer>15</integer>
  </dict>
  <key>StandardOutPath</key>
  <string>${HOME}/ThreadVault/system/maintenance_launchd.out.log</string>
  <key>StandardErrorPath</key>
  <string>${HOME}/ThreadVault/system/maintenance_launchd.err.log</string>
</dict>
</plist>

