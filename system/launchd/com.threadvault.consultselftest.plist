<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.threadvault.consultselftest</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-lc</string>
    <string>source ${HOME}/.threadvault.env 2>/dev/null || true; SELFTEST_NOTIFY=1 ${HOME}/ThreadVault/system/bin/consult-selftest</string>
  </array>
  <key>WorkingDirectory</key>
  <string>${HOME}/ThreadVault</string>
  <key>RunAtLoad</key>
  <true/>
  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>3</integer>
    <key>Minute</key>
    <integer>5</integer>
  </dict>
  <key>StandardOutPath</key>
  <string>${HOME}/ThreadVault/system/consultselftest.out.log</string>
  <key>StandardErrorPath</key>
  <string>${HOME}/ThreadVault/system/consultselftest.err.log</string>
</dict>
</plist>
